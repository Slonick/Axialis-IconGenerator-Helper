                <ResourceDictionary Source="/AxialisIconGeneratorHelper;component/Resources/Styles/Separator.xaml" />
                <ResourceDictionary Source="/AxialisIconGeneratorHelper;component/Resources/Styles/Slider.xaml" />
                <ResourceDictionary Source="/AxialisIconGeneratorHelper;component/Resources/Styles/VideoPlayer.xaml" />
                <ResourceDictionary Source="/AxialisIconGeneratorHelper;component/Resources/Styles/Window.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>